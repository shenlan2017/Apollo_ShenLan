# Define all coms in DAG streaming.
module_config {
    module_library : "/apollo/bazel-bin/modules/localization/shenlan_msf/libshenlan_localization_component.so"

    components {
      class_name : "SLMSFLocalizationComponent"
      config {
        name : "shenlan_localization"
        flag_file_path : "/apollo/modules/localization/conf/localization.conf"
        readers: [
          {
            channel: "/apollo/sensor/gnss/imu"
            qos_profile: {
              depth : 10
            }
            pending_queue_size: 50
          }
        ]
      }
    }
    components {
      class_name : "LoopClosingComponent"
      config {
        name : "shenlan_loop_closing"
        flag_file_path : "/apollo/modules/localization/conf/localization.conf"
        readers: [
          {
            channel: "/apollo/sensor/lidar/compensator/PointCloud2"
            qos_profile: {
              depth : 10
            }
            pending_queue_size: 50
          }
        ]
      }
    }
}
